{
  "name": "@react-pakistan/react-native-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build": "yarn clean:build && yarn build:ts && cp package.json lib && cp README.md lib",
    "build:ts": "tsc --project tsconfig.build.json",
    "bundle": "cd android && ./gradlew bundleRelease",
    "clean": "npx react-native-clean-project",
    "clean:build": "yarn rimraf ./lib",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "format": "yarn format:markdown",
    "format:markdown": "prettier --parser markdown --write *.md",
    "gradle": "cd android; ./gradlew clean; cd ..",
    "ios": "react-native run-ios",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:staged": "lint-staged",
    "lint:css": "stylelint 'src/**/*.ts' --config stylelint.config.js",
    "lint:ts": "eslint --config .eslintrc .",
    "pod:install": "cd ios; pod install; cd ..;",
    "pod:update": "cd ios; pod update; cd ..;",
    "setup": "yarn; yarn pod; yarn gradle;",
    "start": "yarn clean:build && react-native start",
    "test": "jest --coverage --config ./jest.config.js",
    "upgrade": "ncu -u -x react-native-svg"
  },
  "dependencies": {
    "@apollo/client": "3.5.8",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.1.0",
    "@react-navigation/drawer": "^6.2.0",
    "@react-navigation/native": "^6.0.7",
    "@react-navigation/stack": "^6.1.0",
    "@react-pakistan/react-native-commons-collection": "^2.8.4",
    "@react-pakistan/react-native-icon-collection": "^2.5.10",
    "@react-pakistan/react-native-ui-collection": "^2.9.1",
    "@react-pakistan/util-functions": "^1.9.7",
    "@react-pakistan/util-react-native-functions": "^1.6.9",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.25.0",
    "graphql": "^16.3.0",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.4.8",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-reanimated": "^2.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-svg": "^11.0.1",
    "react-native-unimodules": "^0.14.10",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@react-native-community/segmented-control": "^2.2.2",
    "@react-native-community/slider": "^4.2.0",
    "@react-pakistan/eslint-config-shared": "^1.4.12",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-native": "^0.66.15",
    "@types/react-native-onboarding-swiper": "^1.1.4",
    "@types/react-redux": "^7.1.22",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.21",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^27.4.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^19.4.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-circus": "^27.4.6",
    "lint-staged": "^12.3.2",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-native-pager-view": "^5.4.9",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^14.3.0",
    "stylelint-config-react-native-styled-components": "^0.5.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.4.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  }
}
