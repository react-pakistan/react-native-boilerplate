{
  "name": "@react-pakistan/react-native-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build": "yarn clean:build && yarn build:ts && cp package.json lib && cp README.md lib",
    "build:ts": "tsc --project tsconfig.build.json",
    "bundle": "cd android && ./gradlew bundleRelease",
    "clean": "npx react-native-clean-project",
    "clean:build": "yarn rimraf ./lib",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "format": "yarn format:markdown",
    "format:markdown": "prettier --parser markdown --write *.md",
    "gradle": "cd android; ./gradlew clean; cd ..",
    "ios": "react-native run-ios",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:staged": "lint-staged",
    "lint:css": "stylelint 'src/**/*.ts' --config stylelint.config.js",
    "lint:ts": "eslint --config eslint.config.js",
    "pod:install": "cd ios; pod install; cd ..;",
    "pod:update": "cd ios; pod update; cd ..;",
    "setup": "yarn; yarn pod; yarn gradle;",
    "start": "yarn clean:build && react-native start",
    "test": "jest --coverage --config ./jest.config.js",
    "upgrade": "ncu -u -x react-native-svg"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@react-pakistan/react-native-commons-collection": "^2.7.1",
    "@react-pakistan/react-native-icon-collection": "^2.5.8",
    "@react-pakistan/react-native-ui-collection": "^2.8.0",
    "@react-pakistan/util-functions": "^1.7.20",
    "@react-pakistan/util-react-native-functions": "^1.5.33",
    "axios": "^0.24.0",
    "react": "17.0.2",
    "react-native": "0.66.2",
    "react-native-device-info": "^8.4.5",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-onboarding-swiper": "^1.1.4",
    "react-native-reanimated": "^2.2.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-svg": "^11.0.1",
    "react-native-unimodules": "^0.14.10",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.16.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-native-community/segmented-control": "^2.2.2",
    "@react-native-community/slider": "^4.1.11",
    "@react-pakistan/eslint-config-shared": "^1.2.52",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-native": "^0.66.2",
    "@types/react-native-onboarding-swiper": "^1.1.2",
    "@types/react-redux": "^7.1.20",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.15",
    "@types/styled-components-react-native": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "detox": "^19.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-circus": "^27.3.1",
    "lint-staged": "^11.2.6",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-pager-view": "^5.4.8",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^14.0.1",
    "stylelint-config-react-native-styled-components": "^0.4.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.3.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "pre-commit": [
    "lint:staged"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint"
    ]
  }
}
